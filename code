#include<iostream>
using namespace std;

int main() {
	float num1, num2;
	cout << "a couple of numbers please " << endl;
	cin >> num1;
	cin >> num2;
	cout << "division of " << num1 << "by " << num2 << ": " << num1 / num2 << endl;
	if (num1 < num2)
		cout << "in order :" << num1 << num2 << endl;
	else
		cout << "in order :" << num2 << num1 << endl;
	cout << "odd: ";
	if (static_cast<int>(num1) % 2 == 1)
		cout << num1;
	if (static_cast<int>(num2) % 2 == 1)
		cout << num2;
	if (num1 == 13 || num2 == 13)
		cout << "bad luck";
}
